<%= flash[:notice] if flash[:notice] %>
<%= form_for @post, url: posts_path, :html => { :multipart => true } do |f| %>


		<div class= "text">

			
			<%= f.text_field :title, placeholder: "title"%>
			<br>
			<br>
			
			<%= f.text_area :body, placeholder: "Write a post" %>
<br>
<br>
			
			<%= f.file_field :image %>

			
<br>
<br>
			<%= f.submit "Post" %>
			</div>
		<% end %>
